{
    "name": "del001/admin-ui-color-swap",
    "description": "A Magento 2 module to swap the admin UI colors",
    "type": "magento2-module",
    "version": "1.0.2",
    "license": [
        "proprietary"
    ],
    "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/nmarcy-del/m2-AdminUiColorTheme",
          "options": {
              "only-tags": true
          }
      }
  ],
    "require": {
        "php": "~7.4.0||~8.0.0||~8.1.0||~8.2.0",
        "magento/framework": ">=102.0"
    },
    "autoload": {
        "psr-4": {
            "Del001\\AdminUiColorSwap\\": ""
        },
        "files": [
            "registration.php"
        ]
    }
}

